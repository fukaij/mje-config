# ZSH function file
# Part of the SQL Sybase package
# Fetch and return the list of tables
# For use only by the SQL packages
#
# $Id: _sybase_fetchtables,v 1.1 2001/02/26 18:38:53 mebourne Exp $

{
  echo "set nocount on"
  echo "go"
  if [[ -n $_sql_parameters[database] ]]
  then
    echo "use $_sql_parameters[database]"
    echo "go"
  fi
  echo "select name from sysobjects where type in ('U', 'V', 'S') and uid=user_id()"
  echo "go"
} | _sybase_rawsql | tail +3
