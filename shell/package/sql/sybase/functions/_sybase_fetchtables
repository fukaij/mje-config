# ZSH function file
# Part of the SQL Sybase package
# Fetch and return the list of tables
# For use only by the SQL packages
#
# $Id: _sybase_fetchtables,v 1.3 2001/07/02 13:55:17 mebourne Exp $

{
  echo "set nocount on"
  echo "go"
  echo "select name from sysobjects where type in ('U', 'V', 'S')"
  echo "go"
} | _sybase_rawsql | tail +3
