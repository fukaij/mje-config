# ZSH function file
# Part of the SQL Oracle package
# Fetch and return the list of tables
# For use only by the SQL packages
#
# $Id: _oracle_fetchtables,v 1.4 2002/05/24 16:14:15 mebourne Exp $

{
  echo "set pages 0 feed off"
  echo "select object_name from ${_sql_parameters[access]:-user}_objects"
  echo "where object_type in ('TABLE', 'VIEW');"
} | _oracle_rawsql
