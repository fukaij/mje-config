{
  # Handle count & maxrows options here

  echo "alter session set nls_date_format='YYYY/MM/DD HH24:MI:SS';"
  echo "set linesize 1000 pagesize 10000 tab off colsep '\xa0' feedback on;"
  echo - "$argv[*] ;"
} | _oracle_interactive -S
