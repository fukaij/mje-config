# ZSH function file
# Part of the SQL Oracle package
# Fetch and return the list of functions
# For use only by the SQL packages
#
# $Id: _oracle_fetchfuncs,v 1.2 2002/05/24 16:14:03 mebourne Exp $

{
  echo "set pages 0 feed off"
  echo "select object_name from ${_sql_parameters[access]:-user}_objects"
  echo "where object_type='FUNCTION';"
} | _oracle_rawsql
