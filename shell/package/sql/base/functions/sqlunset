# ZSH function file
# Part of the SQL base package
# Unset sql variables
# Usage: sqlunset <sql-variable> ...
#
# $Id: sqlunset 792 2003-09-22 11:47:18Z martin $

eval $_sql_intro

if (( ARGC ))
then
  local var=""
  for var in $argv
  do
    unset "_sql_parameters[$var]"
  done
else
  echo "Syntax: sqlunset <sql-variable> ..."
  return 1
fi
