# ZSH function file
# Automatic completer for functions which use parse_opts
#
# $Id: _parse_opts,v 1.1 2001/07/31 17:16:40 mebourne Exp $

_parse_opts() {
  local command="$words[1]"

  local toeval="$($command --zsh-completion 2>/dev/null)"
  if [[ -n $toeval ]]
  then
    eval $toeval
  else
    _message "error executing '$command --zsh-completion'"
  fi
}

_parse_opts "$argv[@]"
