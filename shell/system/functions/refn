# ZSH function file
# Register new autoload function. Re-registers if already defined
# Usage: refn <function-name>
#
# $Id: refn,v 1.3 2001/02/13 15:25:02 mebourne Exp $

local fn=$1

[[ -z $1 ]] && { echo "Syntax: refn <function-name>" 1>&2; return 1 }

unfunction - $1 2>/dev/null
autoload -U - $1
