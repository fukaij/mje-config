# ZSH configuration file
# Executed for interactive shells via zshrc
# Shell option settings
# Written by Martin Ebourne
#
# $Id: options,v 1.4 2001/03/16 15:52:43 mebourne Exp $

setopt append_history
setopt auto_cd
unsetopt auto_menu
setopt cdable_vars
setopt complete_in_word
setopt correct_all
setopt csh_null_glob
setopt extended_glob
setopt extended_history
setopt hist_find_no_dups
setopt hist_ignore_all_dups
# setopt hist_ignore_space  This taken out due to prompt system emboldening command
setopt hist_no_store
# setopt hist_reduce_blanks  This taken out due to prompt system emboldening command
setopt hist_save_no_dups
setopt interactive_comments
setopt ksh_option_print
setopt list_packed
setopt long_list_jobs
setopt mail_warning
setopt mark_dirs
setopt numeric_glob_sort
setopt path_dirs
setopt print_exit_value
setopt prompt_subst
setopt rc_quotes
setopt rm_star_silent
setopt share_history
